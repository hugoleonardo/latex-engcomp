%% IMPORTANTE!!
%% A acentuação neste arquivo deve ser feita por comandos:
%% Exemplo: Para escrever Ártico, utilize \'{A}rtico
%% Isso garante a ordem alfabética.
%% =====================================================

%% Exemplo de como colocar um livro
@BOOK{ref1,
  title = {T\'{\i}tulo Teste},
  publisher = {Editora},
  year = {2007},
  author = {Nome Nomedomeio Sobrenome},
  volume = {3},
  address = {Rio de Janeiro},
  edition = {12}
}

%% Exemplo de como colocar um artigo de jornal sem autor
@article{ref2,
address={São Paulo},
journal={A Folha de S.~Paulo},
month={2 abr.},
pages={4},
title={Nome do artigo},
year={1995}
}

%% Exemplo de como colocar um artigo de jornal com autor
@article{ref3,
author={Nome Autor},
address={São Paulo},
journal={A Folha de S.~Paulo},
month={7 set.},
pages={11-23},
title={Título do artigo},
year={1995}
}

%% Exemplo de como colocar um artigo de revista sem autor
@article{ref4,
address={Rio de Janeiro},
journal={Nome da revista},
month={jan.},
number={35},
pages={51-60},
title={T\'{\i}tulo do Artigo},
year={1987}}

%% Mais um exemplo de como colocar um livro, mas com subtítulo
@book{ref5,
address={São Paulo},
author={Nome Autor},
publisher={Editora},
subtitle={Subtítudo, que vem depois de dois pontos},
title={Título},
year={1995}}

%% Mais um exemplo de como colocar um livro, mas indicando as páginas.
@book{ref6,
pages={101-114},
author={Outro Nome},
title={Algum nome},
year={1978}}

%% Mais um exemplo de como colocar um livro, mas com vários autores.
@book{ref7,
author={X. Y. Z. Evans and Outro Autor and Mais Um and W. S. Caule},
title={Exemplo de citação no texto},
year={1987}}

%% Um exemplo de como citar um manual de algum grupo ou organização, obtido na internet.
@manual{ref8,
author={X. Y. Silva},
organization={Grupo de Desenvolvimento Imaginário},
title={T\'{\i}tulo de exemplo},
url={http://nomedodominio.com.br},
year={2003},
urlaccessdate={8 mar. 1999}}

%% Um exemplo de como citar o texto de alguma página da internet sem autor
@article{ref9,
title={Conceitos criados como exemplo},
url={http://nomedodominio.com.br},
year={2003},
urlaccessdate={8 mar. 1999}}

